-- function F1 return Integer
  (V1_Fail_1) F1 (1) => False
  (V1_Fail_2) F1 (I => 1) => False

-- function F2 (I : Integer) return Integer
  (V2_OK_1) F2 (1) => True
  (V2_OK_2) F2 (I => 1) => True
  (V2_Fail_3) F2 (1, 2) => False
  (V2_Fail_4) F2 (J => 1) => False
  (V2_Fail_5) F2 (1, I => 2) => False
  (V2_Fail_6) F2 (J => 1) => False

-- function F3 (I, J : Integer) return Integer
  (V3_OK_1) F3 (1, 2) => True
  (V3_OK_2) F3 (1, J => 2) => True
  (V3_OK_3) F3 (J => 1, I => 2) => True
  (V3_Fail_1) F3 (1) => False
  (V3_Fail_2) F3 (1, 2, 3) => False
  (V3_Fail_3) F3 (I => 1) => False
  (V3_Fail_4) F3 (J => 1) => False
  (V3_Fail_5) F3 (1, I => 2) => True
  (V3_Fail_6) F3 (1, I => 1, J => 2) => False

-- function F4 (I : Integer; J : Integer := 0) return Integer
  (V4_OK_1) F4 (1, 2) => True
  (V4_OK_2) F4 (1) => True
  (V4_OK_3) F4 (I => 1) => True
  (V4_OK_4) F4 (I => 1, J => 2) => True
  (V4_OK_5) F4 (J => 1, I => 2) => True
  (V4_Fail_1) F4 (J => 1) => False
  (V4_Fail_2) F4 (1, J => 1, I => 2) => False
