IfStatement(BinOp(NameExpr([NameComponent([Id(Temperature)])]), gte, Num(40.0)), [NameExpr([NameComponent([CallExpr(Id(Put_Line), [ParamList([(None, NameExpr([NameComponent([Str("Wow")])]))])])])]), NameExpr([NameComponent([CallExpr(Id(Put_Line), [ParamList([(None, NameExpr([NameComponent([Str("It's extremely hot")])]))])])])])], [], None)
